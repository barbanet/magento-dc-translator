<config>
    <modules>
        <Dc_Dc>
            <version>1.0.0</version>
        </Dc_Dc>
    </modules>
    <adminhtml>
		<translate>
            <modules>
                <Dc_Dc>
                    <files>
                        <default>Dc_Dc.csv</default>
                    </files>
                </Dc_Dc>
            </modules>
        </translate>
        <layout>
            <updates>
                <dc>
                    <file>dc/dc.xml</file>
                </dc>
            </updates>
        </layout>
    </adminhtml> 
	<global>
	    <blocks>
	        <dc>
	            <class>Dc_Dc_Block</class>
            </dc>
        </blocks>
        <helpers>
            <dc>
                <class>Dc_Dc_Helper</class>
            </dc>
        </helpers>
    </global>
</config>
